{
    "name": "yethee/enum-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a typed enumeration for Symfony2 applications",
    "keywords": ["typed enumeration"],
    "license": "MIT",
    "authors": [
        { "name": "Denis Vasilev", "email": "liquid.yethee@hotmail.com" }
    ],

    "require": {
        "php":             ">=5.3.2",
        "symfony/symfony": "2.1.*"
    },

    "require-dev": {
        "jms/serializer-bundle": "dev-master",
        "doctrine/common":       ">=2.0"
    },

    "suggest": {
        "jms/serializer-bundle": "*"
    },

    "autoload": {
        "psr-0": { "Biplane\\EnumBundle": "" }
    },

    "target-dir": "Biplane/EnumBundle"
}